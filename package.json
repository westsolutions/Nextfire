{
  "name": "tribesocial",
  "version": "1.0.74",
  "description": "Live streaming app",
  "main": "index.js",
  "engines": {
    "node": ">11.0.0"
  },
  "scripts": {
    "dev": "next",
    "dev:local": "npm-run-all --parallel backend dev",
    "backend": "NODE_ENV=development next dev",
    "backend:inspect": "NODE_OPTIONS='--inspect' NODE_ENV=development next dev",
    "build": "next build",
    "start": "NODE_ENV=production next start -p $PORT",
    "export": "next export",
    "format": "prettier --write \"src/**/*.+(js|ts|tsx|scss)\"",
    "upgrage-version": "npm --no-git-tag-version version patch; git commit -n -a -m \"update version\""
  },
  "repository": {
    "type": "git",
    "url": "git@git.geekyants.com:products/nextjs-typescript-firebase.git"
  },
  "author": "Taras Kumpanenko <taras.kumpanenko@gmail.com> ",
  "license": "ISC",
  "dependencies": {
    "@types/next": "^7.0.6",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.1.7",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "cookies-js": "^1.2.3",
    "cors": "^2.8.5",
    "date-fns": "^2.11.0",
    "dompurify": "^2.0.8",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "emoji-mart": "^2.11.2",
    "es6-promise": "^4.2.8",
    "firebase": "^7.9.3",
    "formik": "^2.1.4",
    "husky": "^4.2.3",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.1",
    "next": "9.2.2",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-file-utils": "0.3.13",
    "react-loader": "^2.4.7",
    "react-redux": "^6.0.0",
    "react-spinners": "^0.4.8",
    "reactfire": "2.0.0-rc.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "stream-chat-react": "1.1.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3",
    "yup": "^0.28.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged; npm run upgrage-version"
    }
  }
}
